# Programs
alias less=$PAGER
alias zless=$PAGER
alias kd='source kd.sh'
alias g='git status'

# Filesystem nav
alias dave='cd ~/Development'
alias cdb='cd ..'
alias dl='cd ~/Downloads'
alias webapp="cd ~/Development/webapp"

function mkcd {
  mkdir $1
  cd $1
}

cd(){
  builtin cd "$@" && ls
}

alias rmd='rm -r'

# Utils
alias reload='source ~/.bashrc'
